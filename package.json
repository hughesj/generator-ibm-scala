{
  "name": "generator-ibm-scala",
  "version": "0.0.4",
  "description": "Generate a Scala project",
  "homepage": "",
  "author": {
    "name": "Jeremy Hughes",
    "email": "Jeremy.Hughes@uk.ibm.com",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "scala",
    "ibm",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^6.1.1",
    "nsp": "^2.8.0",
    "prettier": "^1.13.7",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.8.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "chalk": "^2.1.0",
    "handlebars": "^4.7.4",
    "time-require": "^0.1.2",
    "yeoman-generator": "^5.0.0",
    "yosay": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/hughesj/generator-ibm-scala.git",
  "license": "Apache-2.0"
}
